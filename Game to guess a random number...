empty_list = []
for i in range(1,101):
    empty_list.append(str(i))
empty_list = set(empty_list)
empty_list = list(empty_list)
random_num = int(empty_list[0])
for i in range(1,6):    
    value=int(input('Enter you value: '))
    if value==random_num:
        print(' You win!')
        break
    elif value < random_num:
        print("Your guess is too low.")
    else:
        print("Your guess is too high.")

    print(f"Now your attempts left {5-i} ...")
else:
    print(f"Sorry, you lost! The correct number was {random_num}.")
